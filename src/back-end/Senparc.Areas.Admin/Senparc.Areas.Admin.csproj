		<PackageReference Include="Senparc.Ncf.AreaBase" Version="0.20.2-preview1" />
		<PackageReference Include="Senparc.Ncf.Core" Version="0.23.4-preview1" />
		<PackageReference Include="Senparc.Ncf.Log" Version="0.19.1-preview1" />
		<PackageReference Include="Senparc.Ncf.Mvc.UI" Version="0.20.2-preview1" />
		<PackageReference Include="Senparc.Ncf.Service" Version="0.22.1-preview1" />
		<PackageReference Include="Senparc.Ncf.Utility" Version="0.19.1-preview1" />
		<PackageReference Include="Senparc.Ncf.XncfBase" Version="0.22.1-preview1" />

